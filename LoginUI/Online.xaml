<Page x:Class="MCLauncher.LoginUI.Online"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MCLauncher.LoginUI"
      mc:Ignorable="d" 
      d:DesignHeight="200" d:DesignWidth="200"
      Title="Online">
    <Grid>
        <Label VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,150,35" FontSize="16">邮箱</Label>
        <TextBox x:Name="Email" VerticalAlignment="Bottom" Width="150" Height="30" HorizontalAlignment="Right"  FontSize="16" Margin="0,0,0,35"></TextBox>
        <Label VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,150,0" FontSize="16">密码</Label>
        <PasswordBox x:Name="Password" VerticalAlignment="Bottom" Width="150" Height="30" HorizontalAlignment="Right"  FontSize="16" Margin="0,0,0,0"></PasswordBox>
    </Grid>
</Page>